  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@rslib/core": "^0.12.0",
    "@types/node": "^22.17.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-format": "^1.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  }
